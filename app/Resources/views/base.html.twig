<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block pageTitle %}Welcome!{% endblock %}</title>

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

        {% stylesheets '@QuizCoreBundle/Resources/public/css/bootstrap.min.css' %}
        <link rel="stylesheet" href="{{ asset_url }}">
        {% endstylesheets %}




        {% javascripts '@QuizCoreBundle/Resources/public/js/jquery-1.11.2.min.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {% javascripts '@QuizCoreBundle/Resources/public/js/bootstrap.min.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/bmatznerfontawesome/css/font-awesome.min.css') }}" />


        {% stylesheets '@QuizCoreBundle/Resources/public/css/style.css' %}
            <link rel="stylesheet" href="{{ asset_url }}">
        {% endstylesheets %}

        {% stylesheets '@QuizCoreBundle/Resources/public/css/quiz.css' %}
        <link rel="stylesheet" href="{{ asset_url }}">
        {% endstylesheets %}

        {% stylesheets '@QuizCoreBundle/Resources/public/css/sweetalert.css' %}
        <link rel="stylesheet" href="{{ asset_url }}">
        {% endstylesheets %}

        {% block stylesheets %}{% endblock %}
    </head>


    <body>
        {% block body %}{% endblock %}




        {% javascripts '@QuizCoreBundle/Resources/public/js/jquery.color.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {% javascripts '@QuizCoreBundle/Resources/public/js/knockout.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {% javascripts '@QuizCoreBundle/Resources/public/js/knockout.mapping.js' %}
             <script src="{{ asset_url }}"></script>
        {% endjavascripts %}


        {% javascripts '@QuizCoreBundle/Resources/public/js/scripts.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}


        {% javascripts '@QuizCoreBundle/Resources/public/js/lodash.js' %}
            <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {% javascripts '@QuizCoreBundle/Resources/public/js/sweetalert.min.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {% block javascripts %}{% endblock %}
    </body>
</html>
